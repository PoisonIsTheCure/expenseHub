[production]
# Replace with your Digital Ocean droplet IP address
droplet ansible_host=YOUR_DROPLET_IP ansible_user=root ansible_python_interpreter=/usr/bin/python3

[production:vars]
ansible_ssh_private_key_file=~/.ssh/id_rsa
# Or specify your DO SSH key path
# ansible_ssh_private_key_file=~/.ssh/digitalocean_rsa

# If using non-standard SSH port
# ansible_port=2222

